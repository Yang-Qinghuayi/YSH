{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "YSH",
  "version": "0.1.0",
  "identifier": "com.yqhy.ysh",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "YSH",
        "width": 1200,
        "height": 1000,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/yqhy_ic.png"]
  }
}
